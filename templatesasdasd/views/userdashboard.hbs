    if (currentTime >= 9 && currentTime < 11) {
      greetingElement.textContent = 'Good Morning, Vivek Have a Nice Day!';
    } else if (currentTime >= 11 && currentTime < 16) {
      greetingElement.textContent = 'Good Afternoon, Vivek Have a Nice Day!';

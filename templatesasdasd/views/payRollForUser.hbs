<script>
    function freezeAllSalaries() {
        // Assuming you are using jQuery for AJAX
        $.ajax({
            type: 'POST',
            url: '/freezeAllSalaries',
            success: function (response) {
                // Handle the response from the server if needed
                console.log(response);

                // Show SweetAlert success message
                Swal.fire({
                    icon: 'success',
                    title: 'Success',
                    text: 'All salaries frozen successfully',
                });

            },
            error: function (error) {
                // Handle errors if any
                console.error(error);

                // Show SweetAlert error message
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Failed to freeze salaries. Please try again.',

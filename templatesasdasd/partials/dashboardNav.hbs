      height: 50px;
    }

    .calendar-week-day div {
      display: grid;
      place-items: center;
      color: #000;
    }

    .calendar-day {
      display: grid;
      grid-template-columns: repeat(7, 1fr);
      gap: 2px;
      color: #000;
    }

    .calendar-day div {
      width: 5.5vw;
      height: 40px;
      display: flex;
      align-items: center;
      justify-content: center;
